    "sonner": "2.0.7",
    "tailwind-merge": "3.3.1"
  },
  "devDependencies": {
    "@types/node": "24.7.1",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.1",
    "@vitejs/plugin-react-swc": "4.1.0",
    "gh-pages": "^6.3.0",
    "prettier": "3.6.2",
